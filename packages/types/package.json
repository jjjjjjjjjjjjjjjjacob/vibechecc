{
  "name": "@vibechecc/types",
  "version": "1.0.0",
  "description": "Shared TypeScript types for the vibechecc workspace",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm && tsc --project tsconfig.build.json --emitDeclarationOnly",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
