{
  "name": "@vibechecc/utils",
  "version": "0.1.0-beta.0",
  "description": "Shared utility functions for the vibechecc workspace",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.mjs",
      "require": "./dist/constants/index.js"
    },
    "./format": {
      "types": "./dist/format/index.d.ts",
      "import": "./dist/format/index.mjs",
      "require": "./dist/format/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/constants/index.ts src/format/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/constants/index.ts src/format/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
