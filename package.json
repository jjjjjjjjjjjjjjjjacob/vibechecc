    "@tanstack/react-router-with-query": "^1.120.10",
    "@tanstack/react-start": "^1.120.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "concurrently": "^8.2.2",
    "convex": "^1.24.8",
    "framer-motion": "^12.15.0",
    "ky": "^1.7.4",
